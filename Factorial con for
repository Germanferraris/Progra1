// Factorial.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std; 
void main()
{
	int n,p=1,i;
	do{
		cout<<"Ingrese un numero ";
		cin>> n;
	} while (n<0);
	for (i=1;i<=n; i++)
		p=p*i;
	
	cout<< "El factorial de "<< n <<" es: "<<p;
	getch();
}
